{
  "permissions": {
    "allow": [
      "Bash(git rm:*)",
      "Bash(git add:*)",
      "Bash(for file in \"src/pages/signup-minimal.tsx\" \"src/pages/signup-production.tsx\" \"src/pages/signin-simple.tsx\")",
      "Bash(do)",
      "Bash(if [ -f \"$file\" ])",
