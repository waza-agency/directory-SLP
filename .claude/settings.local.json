      "Bash(done)",
      "Bash(git commit:*)",
      "Bash(git push:*)",
      "Bash(node scripts/update-cafes-post-images.js:*)"
    ]
  },
  "enableAllProjectMcpServers": true,
  "enabledMcpjsonServers": [
    "playwright"
  ]
}
