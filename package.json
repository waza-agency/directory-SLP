  "scripts": {
    "dev": "node server.js",
    "dev:next": "next dev",
    "build": "next build",
    "netlify-build": "node scripts/netlify-build.js",
    "export": "next export",
    "build-static": "next build && next export",
    "start": "node server.js",
    "start:next": "next start",
    "lint": "next lint",
    "preinstall": "node -e \"try { require('./scripts/ensure-node-compatibility.js') } catch(e) { console.log('Skipping version check') }\"",
